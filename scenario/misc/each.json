{
	"type" : "each",
	"value" : "threads",
	"in" : [
		1, 10
	],
	"config" : {
		"load" : {
			"limit" : {
				"count" : 1000,
				"time" : "100s"
			},
			"threads" : "${threads}"
		}
	},
	"jobs" : [
		{
			"type" : "each",
			"value" : "size",
			"in" : [
				"4KB", "4MB"
			],
			"config" : {
				"item" : {
					"data" : {
						"size" : "${size}"
					},
					"dst" : {
						"container" : "items_${size}_${threads}"
					}
				}
			},
			"jobs" : [
				{
					"type" : "load",
					"config" : {
						"item" : {
							"dst" : {
								"file" : "items_${size}_${threads}"
							}
						},
						"load" : {
							"type" : "create"
						}
					}
				/*}, {
					"type" : "load",
					"config" : {
						"item" : {
							"data" : {
								"ranges" : 5
							}
						},
						"load" : {
							"type" : "update"
						}
					}*/
				}, {
					"type" : "load",
					"config" : {
						"item" : {
							"src" : {
								"file" : "items_${size}_${threads}"
							}
						},
						"load" : {
							"type" : "read"
						}
					}
				}, {
					"type" : "load",
					"config" : {
						"item" : {
							"src" : {
								"file" : "items_${size}_${threads}"
							}
						},
						"load" : {
							"type" : "delete"
						}
					}
				}
			]
		}
	]
}