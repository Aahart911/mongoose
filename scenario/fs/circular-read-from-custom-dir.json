{
	"type" : "sequential",
	"config" : {
		"item" : {
			"dst" : {
				"container" : "/tmp/scenario-fs-circular-read-from-custom-dir"
			},
			"src" : {
				"container" : "/tmp/scenario-fs-circular-read-from-custom-dir"
			}
		},
		"load" : {
			"limit" : {
				"count" : 10
			}
		},
		"storage" : {
			"type" : "fs"
		}
	},
	"jobs": [
		{
			"type": "load"
		}, {
			"type": "load",
			"config" : {
				"load" : {
					"type" : "read",
					"circular" : true,
					"limit" : {
						// count -> infinity, time -> 1 minute
						"count" : 0,
						"time" : "1m"
					}
				}
			}
		}
	]
}

