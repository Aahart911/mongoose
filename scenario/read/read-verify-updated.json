{
	"type" : "sequential",
	"config" : {
		"container" : {
			"name" : "linear-read-verify-updated"
		},
		"data" : {
			"size" : "1KB"
		},
		"load" : {
			"limit" : {
				"count" : 100
			}
		}
	},
	"jobs": [
		{
			"type": "load", // write is used by default
			"config" : {
				"item" : {
					"dst" : {
						"file" : "/tmp/scenario-linear-read-verify-updated-0.csv"
					}
				}
			}
		}, {
			"type": "load",
			"config" : {
				"item" : {
					"data" : {
						"ranges" : 1 // update 1 random byte ranges per request
					},
					"dst" : {
						"file" : "/tmp/scenario-linear-read-verify-updated-1.csv"
					},
					"src" : {
						"file" : "/tmp/scenario-linear-read-verify-updated-0.csv"
					}
				}
			}
		}, {
			"type": "load",
			"config" : {
				"item" : {
					"src" : {
						"file" : "/tmp/scenario-linear-read-verify-updated-1.csv"
					}
				},
				"load" : {
					"type" : "read"
				}
			}
		}
	]
}

