{
	"type" : "sequential",
	"config" : {
		"item" : {
			"container" : {
				"name" : "scenario-copy-swift-object"
			},
			"data" : {
				"size" : "1KB"
			}
		},
		"storage" : {
			"http" : {
				"api" : "swift",
				"namespace" : "examplens"
			}
		}
	},
	"jobs" : [
		{
			"type" : "load",
			"config" : {
				"item" : {
					"dst" : {
						"file" : "/tmp/scenario-copy-swift-object.json"
					}
				},
				"load" : {
					"limit" : {
						"count" : 1
					}
				}
			}
		}, {
			"type" : "load",
			"config" : {
				"item" : {
					"src" : {
						"file" : "/tmp/scenario-copy-swift-object.json"
					}
				},
				"load" : {
					"copy" : true,
					"limit" : {
						"count" : 1000
					},
					"load" : {
						"threads" : 100
					}
				}
			}
		}
	]
}
