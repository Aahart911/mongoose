{
	"type" : "sequential",
	"config" : {
		"item" : {
			"container" : {
				"name" : "linear-delete"
			},
			"load" : {
				"limit" : {
					"count" : 1000
				}
			}
		}
	},
	"jobs": [
		{
			"type": "load", // write is used by default
			"config" : {
				"item" : {
					"dst" : {
						"file" : "/tmp/scenario-linear-delete.csv"
					}
				}
			}
		},
		{
			"type": "load",
			"config" : {
				"item" : {
					"src" : {
						"file" : "/tmp/scenario-linear-delete.csv"
					}
				},
				"load" : {
					"type" : "delete"
				}
			}
		}
	]
}
