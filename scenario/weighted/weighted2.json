{
	"type" : "sequential",
	"config" : {
		"item" : {
			"data" : {
				"size" : "8KB"
			},
			"dst" : {
				"container" : "weighted2-load",
				"file" : "/tmp/scenario-weighted2-read-input.csv"
			}
		},
		"load" : {
			"limit" : {
				"count" : 10000
			},
			"metricsPeriod" : 0,
			"threads" : 100
		}
	},
	"jobs" : [
		{
			"type" : "load",
			"config" : {
				"item" : {
					"data" : {
						"size" : "8KB"
					},
					"dst" : {
						"file" : "/tmp/scenario-weighted2-read-input.csv"
					}
				}
			}
		}, {
			"type" : "load",
			"config" : {
				"item" : {
					"data" : {
						"size" : "1KB"
					},
					"dst" : {
						"file" : "/tmp/scenario-weighted2-delete-input.csv"
					}
				}
			}
		}, {
			"type" : "load",
			"config" : {
				"item" : {
					"data" : {
						"size" : "64KB"
					},
					"src" : {
						// new item generator will be used for write,
						// one CSV file will be used for read,
						// and the other CSV file will be used for delete
						"file" : [
							null,
							"/tmp/scenario-weighted2-read-input.csv",
							"/tmp/scenario-weighted2-delete-input.csv"
						]
					}
				},
				"load" : {
					"circular" : true,
					"limit" : {
						"time" : "5m",
						"count" : 0
					},
					"metricsPeriod" : 10,
					"type" : [
						"write=50%", "read=30%", "delete=20%"
					]
				}
			}
		}
	]
}
