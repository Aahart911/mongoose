{
	"configuration": {
		"status": "off",
		"name": "mongoose",
		"shutdownHook": "disable",
		"appenders": {
			"appender" : [
				{
					"name": "stdout",
					"type": "Console",
					"follow": true,
					"PatternLayout": {
						"alwaysWriteExceptions" : false,
						"pattern": "%highlight{%d{ISO8601}{GMT+0} %p{length=1} %-25.25c{1} %-25.25t %m%n}"
					},
					"Filters": {
						"Filter" : [
							{
								"type": "MarkerFilter",
								"marker": "cfg",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "dataList",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfTrace",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "ThresholdFilter",
								"level": "info",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "async",
					"type": "Async",
					"blocking": "true",
					"bufferSize": "1000000",
					"AppenderRef": { "ref": "stdout" }
				}
			]
		},
		"loggers": {
			"root": {
				"level": "debug",
				"additivity": "false",
				"AppenderRef": {
					"ref": "async"
				}
			},
			"logger": [
				{
					"name": "com.emc.mongoose.client",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.common",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.run",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.server",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.storage.adapter.atmos",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.storage.adapter.s3",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.storage.adapter.swift",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.storage.mock",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.util",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.webui",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "org.python.core.PyReflectedFunction",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}
			]
		}
	}
}
