{
	"configuration": {
		"status": "info",
		"name": "mongoose",
		"monitorInterval": "1000",
		"properties": {
			"property": {
				"name": "path.log",
				"value": "${sys:dir.root}${sys:file.separator}log${sys:file.separator}${sys:run.mode}${sys:file.separator}${sys:run.id}${sys:file.separator}"
			}
		},
		"shutdownHook": "disable",
		"appenders": {
			"appender" : [
				{
					"name": "WebUI",
					"type": "WebUI",
					"enabled": "true",
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "dataList",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfTrace",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "ThresholdFilter",
								"level": "INFO",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "stdout",
					"type": "Console",
					"PatternLayout": {
						"pattern": "%highlight{%d{ISO8601}{GMT+0} %p{length=1} %-25.25c{1} %-45.45t %m%n}"
					},
					"Filters": {
						"Filter" : [
							{
								"type": "MarkerFilter",
								"marker": "dataList",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfTrace",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "ThresholdFilter",
								"level": "INFO",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "msgFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}messages.csv",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %p | %c{1} | %t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "msg",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "3rdPartyFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}3rdparty.log",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %p | %c{1} | %t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "msg",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "err",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "dataList",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfAvg",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfSum",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfTrace",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "ThresholdFilter",
								"level": "INFO",
								"onMatch": "NEUTRAL",
								"onMismatch": "DENY"
							}, {
								"type": "BurstFilter",
								"rate": 10,
								"maxBurst": 100
							}
						]
					}
				}, {
					"name": "errFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}errors.log",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %p | %c{1} | %t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "err",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}, {
								"type": "BurstFilter",
								"rate": 10,
								"maxBurst": 100
							}
						]
					}
				}, {
					"name": "perfAvgFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}perf.avg.csv",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %-1p | %c{1} | %32t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "perfAvg",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "perfSumFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}perf.sum.csv",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %-1p | %c{1} | %t | %m%n"
					},
					"MarkerFilter": {
						"marker": "perfSum",
						"onMatch": "ACCEPT",
						"onMismatch": "DENY"
					}
				}, {
					"name": "perfTraceFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}perf.trace.csv",
					"PatternLayout": { "pattern": "%t,%m%n" },
					"MarkerFilter": {
						"marker": "perfTrace",
						"onMatch": "ACCEPT",
						"onMismatch": "DENY"
					}
				}, {
					"name": "dataListFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}data.items.csv",
					"PatternLayout": { "pattern": "%m%n" },
					"MarkerFilter": {
						"marker": "dataList",
						"onMatch": "ACCEPT",
						"onMismatch": "DENY"
					}
				}, {
					"name": "async",
					"type": "Async",
					"blocking": "false",
					"bufferSize": "131072",
					"AppenderRef": [
						{ "ref": "stdout" },
						{ "ref": "msgFile" },
						{ "ref": "3rdPartyFile" },
						{ "ref": "errFile" },
						{ "ref": "dataListFile" },
						{ "ref": "perfAvgFile" },
						{ "ref": "perfSumFile" },
						{ "ref": "perfTraceFile" },
						{ "ref": "WebUI" }
					]
				}
			]
		},
		"loggers": {
			"root": {
				"level": "debug",
				"additivity": "false",
				"AppenderRef": {
					"ref": "async"
				}
			},
			"logger": [
				// mongoose load client packages
				{
					"name": "com.emc.mongoose.client",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.client.impl.load.builder",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.client.impl.load.executor",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.client.impl.load.executor.gauges",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.client.impl.load.executor.tasks",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.client.impl.persist",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				},

				// mongoose core packages
				{
					"name": "com.emc.mongoose.core",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.data",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.data.src",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.io.req",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.io.req.conf",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.io.task",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.io.util",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.load.builder",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.load.executor",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.load.executor.util",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.load.model",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.load.tasks",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.persist",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.core.impl.util",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				},

				// mongoose runners packages
				{
					"name": "com.emc.mongoose.run",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.run.util",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				},

				// mongoose load server packages
				{
					"name": "com.emc.mongoose.server.impl",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.server.impl.load.builder",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.server.impl.load.executor",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.server.impl.persist",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				},

				// mongoose atmos storage adapter
				{
					"name": "com.emc.mongoose.storage.adapter.atmos",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				},

				// mongoose amzs3 storage adapter
				{
					"name": "com.emc.mongoose.storage.adapter.s3",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				},

				// mongoose swift storage adapter
				{
					"name": "com.emc.mongoose.storage.adapter.swift",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				},

				// mongoose storage mock
				{
					"name": "com.emc.mongoose.storage.mock",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.storage.mock.impl.data",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.storage.mock.impl.cinderella",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				},

				// mongoose ui
				{
					"name": "com.emc.mongoose.webui",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.webui.expressions",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.webui.logging",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}, {
					"name": "com.emc.mongoose.webui.websockets",
					"level": "debug", "additivity": "false", "AppenderRef": { "ref": "async" }
				}
			]
		}
	}
}
