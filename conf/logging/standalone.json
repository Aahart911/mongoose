{
	"configuration": {
		"status": "info",
		"name": "mongoose",
		"monitorInterval": "1000",
		"properties": {
			"property": {
				"name": "path.log",
				"value": "${sys:dir.root}${sys:file.separator}log${sys:file.separator}${sys:run.mode}${sys:file.separator}${sys:run.id}${sys:file.separator}"
			}
		},
		"appenders": {
			"appender" : [
				{
					"name": "stdout",
					"type": "Console",
					"PatternLayout": {
						"pattern": "%highlight{%d{ISO8601}{GMT+0} %p{length=1} %-25.25c{1} %-45.45t - %m%n}"
					},
					"Filters": {
						"Filter" : [
							{
								"type": "MarkerFilter",
								"marker": "dataList",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfTrace",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "ThresholdFilter",
								"level": "INFO",
								"onMatch": "ACCEPT"
							}
						]
					}
				}, {
					"name": "msgFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}messages.csv",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %p | %c{1} | %t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "msg",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}, {
								"type": "BurstFilter",
								"rate": 10,
								"maxBurst": 100
							}, {
								"type": "ThresholdFilter",
								"level": "DEBUG",
								"onMatch": "Accept"
							}
						]
					}
				}, {
					"name": "errFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}errors.log",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %p | %c{1} | %t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "err",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}, {
								"type": "BurstFilter",
								"rate": 10,
								"maxBurst": 100
							}
						]
					}
				}, {
					"name": "perfAvgFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}perf.avg.csv",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %-1p | %c{1} | %32t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "perfAvg",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "perfSumFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}perf.sum.csv",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %-1p | %c{1} | %t | %m%n"
					},
					"MarkerFilter": {
						"marker": "perfSum",
						"onMatch": "ACCEPT",
						"onMismatch": "DENY"
					}
				}, {
					"name": "perfTraceFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}perf.trace.csv",
					"PatternLayout": {
						"pattern": "%t,%m%n"
					},
					"MarkerFilter": {
						"marker": "perfTrace",
						"onMatch": "ACCEPT",
						"onMismatch": "DENY"
					}
				},{
                    "name": "dataListFile",
                    "type": "File",
                    "append": "false",
                    "bufferedIO": "true",
                    "fileName": "${path.log}data.items.csv",
                    "PatternLayout": {
                        "pattern": "%m%n"
                    },
                    "MarkerFilter": {
                        "marker": "dataList",
                        "onMatch": "ACCEPT",
                        "onMismatch": "DENY"
                    }
                }, {
                    "name": "testDBFile",
                    "type": "File",
                    "append": "false",
                    "bufferedIO": "true",
                    "fileName": "${path.log}testDB.log",
                    "PatternLayout": {
                        "pattern": "INSERT INTO TABLE messages(run,level,class,tstamp,message) values (${sys:run.id}, %-5p, %-16.16c{1}, %d{ISO8601}{GMT+0}, %m%n"
                    },
                    "MarkerFilter": {
                        "marker": "testDB",
                        "onMatch": "ACCEPT",
                        "onMismatch": "DENY"
                    }
                }, {
                    "name": "databaseAppender",
                    "type": "JDBC",
                    "tableName": "test",
                    "ConnectionFactory": {
                        "class": "com.emc.mongoose.util.logging.ConnectionFactory",
                        "method": "getDatabaseConnection"
                    },
                    "Column": [
                        {
                            "name": "run",
                            "pattern": "${sys:run.id}",
                            "isUnicode": "false"
                        }, {
                            "name": "level",
                            "pattern": "%p",
                            "isUnicode": "false"
                        }, {
                            "name": "class",
                            "pattern": "%c",
                            "isUnicode": "false"
                        }, {
                            "name": "tstamp",
                            "isUnicode": "false",
                            "isEventTimestamp": "true"
                        }, {
                            "name": "message",
                            "pattern": "%m",
                            "isUnicode": "false"
                        }
                    ],
                    "MarkerFilter": {
                        "marker": "databaseApp",
                        "onMatch": "ACCEPT",
                        "onMismatch": "DENY"
                    }
                }, {
					"name": "async",
					"type": "Async",
					"blocking": "false",
					"bufferSize": "1048576",
					"AppenderRef": [
						{ "ref": "stdout" },
						{ "ref": "msgFile" },
						{ "ref": "errFile" },
						{ "ref": "dataListFile" },
						{ "ref": "perfAvgFile" },
						{ "ref": "perfSumFile" },
						{ "ref": "perfTraceFile" },
                        { "ref": "testDBFile" },
                        { "ref": "databaseAppender" }
					]
				}
			]
		},
		"loggers": {
			"root": {
				"level": "info",
				"additivity": "false",
				"AppenderRef":
                     { "ref": "async" }

			},
			"logger": [
			     {
					"name": "org.apache.http",
					"level": "info",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "org.apache.http.wire",
					"level": "info",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.base",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.base.api",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.base.data",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.base.data.persist",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.base.load",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.base.load.client",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.object",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.object.api",
					"level": "trace",
					"additivity": "false",
					"AppenderRef":
                         { "ref": "async" }

				}, {
					"name": "com.emc.mongoose.object.api.provider",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.object.api.provider.atmos",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.object.api.provider.s3",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.object.api.provider.swift",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.object.data",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.object.load",
					"level": "trace",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.object.load.type",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.object.load.type.ws",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.run",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.util",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.util.conf",
					"level": "trace",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.util.logging",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.util.pool",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}, {
					"name": "com.emc.mongoose.util.threading",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
                        "ref": "async"
                    }
				}
			]
		}
	}
}
