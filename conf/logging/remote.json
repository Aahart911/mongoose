{
	"configuration": {
		"status": "off",
		"name": "mongoose",
		"monitorInterval": "1000",
		"properties": {
			"property": {
				"name": "path.log",
				"value": "${sys:dir.root}${sys:file.separator}log${sys:file.separator}${sys:run.mode}${sys:file.separator}"
			}
		},
		"shutdownHook": "disable",
		"appenders": {
			"appender" : [
				{
					"name": "WebUI",
					"type": "WebUI",
					"enabled": "true",
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "dataList",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfTrace",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "ThresholdFilter",
								"level": "INFO",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "openJPA",
					"type": "OpenJPA",
					"enabled": "false",
					"database": "postgresql",
					"username": "mongoose",
					"password": "mongoose",
					"addr": "localhost",
					"port": "5432",
					"namedatabase": "mongoose",
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "err",
								"onMatch": "ACCEPT",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "dataList",
								"onMatch": "ACCEPT",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfTrace",
								"onMatch": "ACCEPT",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "msg",
								"onMatch": "ACCEPT",
								"onMismatch": "NEUTRAL"
							}
						]
					}
				}, {
					"name": "stdout",
					"type": "Console",
					"PatternLayout": {
						"pattern": "%highlight{%d{ISO8601}{GMT+0} %p{length=1} %-25.25c{1} %-45.45t %m%n}"
					},
					"Filters": {
						"Filter" : [
							{
								"type": "MarkerFilter",
								"marker": "dataList",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfTrace",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "ThresholdFilter",
								"level": "INFO",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "msgFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}messages.csv",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %p | %c{1} | %t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "BurstFilter",
								"rate": 10,
								"maxBurst": 100
							}, {
								"type": "MarkerFilter",
								"marker": "msg",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "3rdPartyFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}3rdparty.log",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %p | %c{1} | %t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "msg",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "err",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "dataList",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfAvg",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfSum",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "MarkerFilter",
								"marker": "perfTrace",
								"onMatch": "DENY",
								"onMismatch": "NEUTRAL"
							}, {
								"type": "ThresholdFilter",
								"level": "INFO",
								"onMatch": "NEUTRAL",
								"onMismatch": "DENY"
							}, {
								"type": "BurstFilter",
								"rate": 10,
								"maxBurst": 100
							}
						]
					}
				}, {
					"name": "errFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}errors.log",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %p | %c{1} | %t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "err",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "perfAvgFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}perf.avg.csv",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %-1p | %c{1} | %32t | %m%n"
					},
					"Filters": {
						"Filter": [
							{
								"type": "MarkerFilter",
								"marker": "perfAvg",
								"onMatch": "ACCEPT",
								"onMismatch": "DENY"
							}
						]
					}
				}, {
					"name": "perfSumFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}perf.sum.csv",
					"PatternLayout": {
						"pattern": "%d{ISO8601}{GMT+0} | %-1p | %c{1} | %t | %m%n"
					},
					"MarkerFilter": {
						"marker": "perfSum",
						"onMatch": "ACCEPT",
						"onMismatch": "DENY"
					}
				}, {
					"name": "perfTraceFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}perf.trace.csv",
					"PatternLayout": { "pattern": "%t,%m%n" },
					"MarkerFilter": {
						"marker": "perfTrace",
						"onMatch": "ACCEPT",
						"onMismatch": "DENY"
					}
				}, {
					"name": "dataListFile",
					"type": "File",
					"append": "false",
					"bufferedIO": "true",
					"fileName": "${path.log}data.items.csv",
					"PatternLayout": { "pattern": "%m%n" },
					"MarkerFilter": {
						"marker": "dataList",
						"onMatch": "ACCEPT",
						"onMismatch": "DENY"
					}
				}, {
					"name": "async",
					"type": "Async",
					"blocking": "false",
					"bufferSize": "131072",
					"AppenderRef": [
						{ "ref": "stdout" },
						{ "ref": "msgFile" },
						{ "ref": "3rdPartyFile" },
						{ "ref": "errFile" },
						{ "ref": "dataListFile" },
						{ "ref": "perfAvgFile" },
						{ "ref": "perfSumFile" },
						{ "ref": "perfTraceFile" },
						{ "ref": "WebUI" }
					]
				}
			]
		},
		"loggers": {
			"root": {
				"level": "debug",
				"additivity": "false",
				"AppenderRef": {
					"ref": "async"
				}
			},
			"logger": [
